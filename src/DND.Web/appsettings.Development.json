{
  "AppSettings": {
    "Switches": {
      "EnableETags": false
    },
    "SiteUrl": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Debug"
      },
      {
        "Name": "Console",
        "Args": { "outputTemplate": "[{Timestamp:HH:mm:ss.fff}] {Level:u3} - {Message}{NewLine}{Exception}" }
      }
    ]
  },
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "Certificates/localhost.private.pfx",
        "Password": "password"
      }
    }
  }
}
