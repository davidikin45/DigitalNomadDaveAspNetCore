@model dynamic
<div class="row">
    <div class="col-12 mb-2">
        <progressBar percent="75"></progressBar>
    </div>
</div>

<div class="row">
    <div class="col-sm-3 col-lg-2 hidden-xs-down">
        <div class="sticky-top">
            <div class="card">
                <div class="card-body">
                    @await Html.PartialAsync("_DynamicFormMenu")
                </div>
            </div>
        </div>
    </div>
    <div class="col-sm-9 col-lg-10">
        <div class="card">
            <div class="card-body">
                @await Html.PartialAsync("_DynamicFormEdit")
            </div>
        </div>
    </div>
</div>
