@using DND.Common;
@using DND.Common.Extensions;


@model dynamic

@for (int i = 0; i < Model.Count; i++)
{
    var htmlHelper = HtmlHelperExtensions.For(Html, Model[i]);
    @(((IHtmlHelper)htmlHelper).EditorForModel("Object", Html.ViewData.ModelMetadata.Name + "[" + i + "]", new { EditMode = ViewBag.EditMode, CreateMode = ViewBag.CreateMode }));
}