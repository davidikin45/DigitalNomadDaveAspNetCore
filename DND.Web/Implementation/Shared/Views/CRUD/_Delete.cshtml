@model  dynamic

@Html.DisplayForModel()

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-group row">
        <div class="offset-md-2 col-md-10">
            <input type="submit" value="Delete" class="btn btn-warning" onclick="return confirm('Are you sure you want to delete this?');" />
        </div>
    </div>
}